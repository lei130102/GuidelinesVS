        <Polygon Canvas.Left="10" Canvas.Top="175" Stroke="Blue" StrokeThickness="1" Fill="Yellow" FillRule="EvenOdd" Points="15,200 68,70 110,200 0,125 135,125"/>
    </Canvas>
</Window>
